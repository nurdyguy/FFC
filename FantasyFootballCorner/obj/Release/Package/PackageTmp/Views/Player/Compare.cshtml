<script>        
    var seriesData = [];
    var seriesLabels = [];
    var series = [];
    for(var p in graphData)
    {       
        //seriesLabels.push(p);
        //seriesData.push(graphData[p]);     
        series.push({name:p, data:graphData[p]});
    }
    $(function () {
        $('#statChart').highcharts({
            chart: {
                type: 'spline',
                backgroundColor:"#A3A3A3"
            },
            title: {
                text: 'Weely Stats Comparison'
            },
            
            xAxis: {
                type: 'linear',
                title: {
                    text: 'Week'
                },
                ordinal: false,
            },
            yAxis: {
                title: {
                    text: 'Total Passing yards'
                },
                min: 0
            },
            tooltip: {
                headerFormat: '<b>{series.name}</b><br>',
                pointFormat: ' {point.y:.2f} yds'
            },

            plotOptions: {
                spline: {
                    marker: {
                        enabled: true
                    }
                }
            },

            series: series
        });
    });
    

    


    $(function () {
        var tableLen = $('table').find('tr.success').length;
        var html = '<li class="disabled"><a onclick="page(next)">«</a></li>';
        html += '<li class="active"><a onclick="page(this)">1</a></li>';
        for (var i = 2; i < parseInt(tableLen / 10) + 1; i++)
            html += '<li ><a  onclick="page(this)">' + i + '</a></li>';
        html += '<li><a onclick="page(prev)">»</a></li>';

        $('#pager').html(html);

        for (var i = 0; i < 10; i++)
            $('table').find('tr.success').eq(i).show();


        
    });

